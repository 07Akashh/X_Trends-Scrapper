{
    "version": 2,
    "builds": [
        {
            "src": "app/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/index.html",
            "check": true
        },
        {
            "src": "/(.*)",
            "dest": "app/app.js"
        }
    ]
}
