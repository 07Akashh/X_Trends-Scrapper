{
    "version": 2,
    "builds": [
        {
            "src": "app/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1",
            "check": true
        },
        {
            "src": "/api/(.*)",
            "dest": "app/app.js"
        }
    ]
}
