{
    "version": 2,
    "builds": [
        {
            "src": "app/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "app/app.js"
        }
    ],
    "static": {
        "public": "public"
    }
}
